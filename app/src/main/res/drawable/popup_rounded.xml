<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="#00FFFFFF"/>
            <stroke
                android:width="6dp"
                android:color="#ffFFFFFF"/>
        </shape>
    </item>

    <item>
        <shape>
            <solid android:color="#00ffffff" />
