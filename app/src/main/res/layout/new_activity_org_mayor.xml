<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/organization_content"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/new_main_bg_1">

    <RelativeLayout
        android:id="@+id/slide_bar"
        android:layout_width="1296dp"
        android:layout_height="41dp"
        android:layout_alignTop="@+id/container"
        android:layout_marginEnd="75dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="-30dp"
        android:layout_toEndOf="@+id/leader"
        android:background="@drawable/slide_bar"
        android:visibility="visible">

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/leader"
        android:layout_width="465dp"
        android:layout_height="750dp"
        android:layout_alignTop="@+id/container"
        android:layout_marginStart="250dp"
        android:orientation="vertical"
        android:paddingTop="@dimen/activity_vertical_margin">

        <ImageView
            android:id="@+id/leader_box"
            android:layout_width="match_parent"
            android:layout_height="680dp"
            android:src="@drawable/new_member_3_bg"
            android:scaleType="fitXY"
            android:layout_alignTop="@+id/top"
            android:layout_marginTop="5dp"/>

        <LinearLayout
            android:layout_width="455dp"
            android:layout_height="82dp"
            android:background="@drawable/new_title_1_bg_455px"
            android:orientation="horizontal"
            android:id="@+id/title">

            <TextView
                android:id="@+id/leader_team"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="team"
                android:textAlignment="center"
                android:textColor="@android:color/background_light"
                android:textSize="27dp"
                android:gravity="center_vertical"
                android:textStyle="bold"/>
        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="750dp"
            android:orientation="vertical"
            android:id="@+id/top"
            android:layout_below="@id/title">

            <ImageView
                android:id="@+id/work_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/org_title"
                android:layout_marginLeft="415dp"
                android:layout_marginTop="20dp"
                android:src="@drawable/new_led_gray"
                android:visibility="gone"/>

            <ImageView
                android:id="@+id/leader_pic"
                android:layout_width="330dp"
                android:layout_height="450dp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="40dp"
                app:srcCompat="@drawable/no_img_b" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@id/leader_pic"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="20dp"
                android:layout_weight="1"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/leader_name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginRight="15dp"
                    android:layout_marginTop="0dp"
                    android:layout_weight="0.42"
                    android:text="No Data"
                    android:textAlignment="center"
                    android:textColor="#000000"
                    android:textSize="50dp" />

                <TextView
                    android:id="@+id/leader_duty"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/leader_name"
                    android:layout_marginRight="15dp"
                    android:layout_marginTop="7dp"
                    android:layout_weight="0.42"
                    android:text="No Data"
                    android:textAlignment="center"
                    android:textColor="#555555"
                    android:textSize="25dp" />


                <RelativeLayout
                    android:layout_width="260dp"
                    android:layout_height="20dp"
                    android:layout_alignParentBottom="true"
                    android:layout_alignStart="@+id/team_duty"
                    android:layout_marginBottom="15dp"
                    android:layout_marginStart="0dp">

                    <ImageView
                        android:id="@+id/circle"
                        android:layout_width="20dp"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="0dp"
                        android:src="@drawable/org_circle"
                        android:visibility="gone" />

                    <TextView
                        android:id="@+id/leader_mission"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_toEndOf="@+id/circle"
                        android:ellipsize="end"
                        android:maxLines="1"
                        android:text="No Data"
                        android:textColor="#555555"
                        android:textSize="18dp"
                        android:visibility="gone" />
                </RelativeLayout>

                <TextView
                    android:id="@+id/leader_isWorking"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginRight="50dp"
                    android:layout_marginTop="5dp"
                    android:layout_weight="1"
                    android:background="@drawable/org_working_in"
                    android:gravity="center_vertical"
                    android:textAlignment="center"
                    android:textColor="@android:color/background_light"
                    android:textSize="20dp"
                    android:visibility="invisible" />
            </RelativeLayout>
        </RelativeLayout>
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/backbutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/container"
        android:layout_marginBottom="-33dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="0dp">

        <ImageView
            android:layout_width="99dp"
            android:layout_height="99dp"
            android:src="@drawable/new_btn_back" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/seatbutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/container"
        android:layout_marginBottom="-33dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="0dp"
        android:visibility="gone">

        <ImageView
            android:layout_width="120dp"
            android:layout_height="149dp"
            android:scaleType="centerCrop"
            android:src="@drawable/seaticon" />
    </LinearLayout>

    <android.support.v4.view.ViewPager
        android:id="@+id/container"
        android:layout_width="950dp"
        android:layout_height="790dp"
        android:layout_below="@+id/relativeLayout"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="50dp"
        android:layout_toRightOf="@+id/leader"
        android:fadeScrollbars="false"
        android:overScrollMode="never"
        android:scrollbarAlwaysDrawHorizontalTrack="true"
        android:scrollbarSize="4dp"
        android:scrollbarStyle="outsideOverlay"
        android:scrollbarThumbHorizontal="@drawable/viewpager_scroll_thumb"
        android:scrollbars="horizontal" />

    <ImageView
        android:id="@+id/viewpager_shadow"
        android:layout_width="10dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/container"
        android:layout_alignStart="@+id/container"
        android:layout_alignTop="@+id/container"
        android:layout_marginTop="16dp"
        android:src="@drawable/viewpager_shadow"
        android:visibility="invisible" />

    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true">

        <LinearLayout
            android:id="@+id/rightbar"
            style="@style/right_bar_style">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="20dp"
                android:src="@drawable/new_ico_datetime" />

            <lab.nicc.kioskyoungcheon.DigitalClock
                android:id="@+id/digitalClock1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="0dp"
                android:text="DigitalClock"
                android:textColor="#ffffff"
                android:textSize="32dp"
                android:textStyle="bold" />
        </LinearLayout>
    </RelativeLayout> <!-- 시계 -->

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="100dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/title_round"
        android:gravity="center_horizontal"
        android:paddingBottom="15dp"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:paddingTop="15dp">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:paddingTop="0dp">

            <ImageView
                android:id="@+id/gj_logo"
                style="@style/logo_img_style" />

            <ImageView
                android:id="@+id/gj_line"
                android:layout_width="30dp"
                android:layout_height="60dp"
                android:visibility="invisible"
                app:srcCompat="@drawable/logo_line" />

            <TextView
                android:id="@+id/teamLogos"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="-5dp"
                android:fontFamily="@font/nanummyeongjobold"
                android:gravity="center_vertical"
                android:text="Team name"
                android:textColor="#ffffff"
                android:textSize="50sp" />
        </LinearLayout>


        <ImageView
            android:id="@+id/title_home"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="25dp"
            android:layout_marginRight="5dp"
            android:src="@drawable/new_btn_home" />
    </LinearLayout> <!-- 타이틀 -->

    <RelativeLayout
        android:id="@+id/inner_bar"
        android:layout_width="match_parent"
        android:layout_height="115dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/new_mnu_bg">

        <TextView
            android:id="@+id/notice_bar"
            android:layout_width="750dp"
            android:layout_height="60dp"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginStart="34dp"
            android:background="@drawable/notice_round"
            android:ellipsize="end"
            android:gravity="center_vertical"
            android:maxLines="1"
            android:paddingLeft="30dp"
            android:paddingRight="30dp"
            android:text="Notice Title"
            android:textColor="@android:color/background_light"
            android:textSize="30dp" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:paddingTop="15dp">

            <ImageView
                android:id="@+id/bottom_home"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/new_mnu_1_out" />

            <ImageView
                style = "@style/border_style"
                android:src="@drawable/new_mnu_line" />

            <ImageView
                android:id="@+id/bottom_past"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:visibility="gone"
                app:srcCompat="@drawable/new_mnu_2_out" />

            <ImageView
                android:id="@+id/bottom_organization"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/new_mnu_3_over" />

            <ImageView
                style = "@style/border_style"
                android:src="@drawable/new_mnu_line" />

            <ImageView
                android:id="@+id/bottom_building"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/new_mnu_4_out" />

            <ImageView
                style = "@style/border_style"
                android:src="@drawable/new_mnu_line" />

            <ImageView
                android:id="@+id/bottom_video"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/new_mnu_5_out" />

            <ImageView
                style = "@style/border_style"
                android:src="@drawable/new_mnu_line" />

            <ImageView
                android:id="@+id/bottom_gallery"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/new_mnu_6_out" />

            <ImageView
                style = "@style/border_style"
                android:src="@drawable/new_mnu_line" />

            <ImageView
                android:id="@+id/bottom_notice"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@drawable/new_mnu_7_out" />

            <ImageView
                style = "@style/border_style"
                android:src="@drawable/new_mnu_line" />

            <ImageView
                android:id="@+id/bottom_about"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="40dp"
                app:srcCompat="@drawable/new_mnu_8_out" />
        </LinearLayout>
    </RelativeLayout> <!-- 하단 바 -->
    <FrameLayout
        android:id="@+id/seat_popup_parent"
        android:layout_width="1920dp"
        android:layout_height="1080dp"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="0dp"
        android:background="@drawable/seat_top_frame"
        android:visibility="gone">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="76dp"
            android:background="@drawable/bg_top_frame">

        </FrameLayout>

        <FrameLayout
            android:id="@+id/seat_popup"
            android:layout_width="1920dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="0dp"
            android:layout_marginTop="76dp"
            android:background="@drawable/seat_bg01"
            android:visibility="visible">

        </FrameLayout>
    </FrameLayout>

    <TextView
        android:id="@+id/popup_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/seat_popup_parent"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="9dp"
        android:text="좌석배치도"
        android:textColor="#ffffff"
        android:textSize="45sp"
        android:textStyle="normal"
        android:visibility="gone" />

    <Button
        android:id="@+id/seat_close"
        android:layout_width="68dp"
        android:layout_height="49dp"
        android:layout_alignEnd="@+id/seat_popup_parent"
        android:layout_alignTop="@+id/seat_popup_parent"
        android:layout_marginRight="24dp"
        android:layout_marginTop="13dp"
        android:background="@drawable/bg_close1"
        android:visibility="gone" />

    <FrameLayout
        android:id="@+id/dark"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="#66000000"
        android:visibility="gone" />

    <RelativeLayout
        android:id="@+id/popup"
        android:layout_width="1000dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:visibility="gone">

        <LinearLayout
            android:id="@+id/pop"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:orientation="vertical"
            android:visibility="visible">

            <LinearLayout
                android:id="@+id/card"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/new_id_card_bg"
                android:orientation="vertical"
                android:paddingTop="340dp">

                <LinearLayout
                    android:layout_width="489dp"
                    android:layout_height="240dp"
                    android:layout_marginLeft="48dp"
                    android:layout_marginTop="98dp">

                    <ImageView
                        android:id="@+id/popup_pic"
                        android:layout_width="150dp"
                        android:layout_height="205dp"
                        android:layout_marginTop="0dp"
                        android:scaleType="fitXY"
                        app:srcCompat="@drawable/a" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="0dp"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="44dp"
                            android:layout_marginLeft="15dp"
                            android:gravity="center_vertical">

                            <TextView
                                android:id="@+id/popup_name"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="-7dp"
                                android:fontFamily="@font/nanumsquareeb"
                                android:text="한상길"
                                android:textColor="#373737"
                                android:textSize="35sp" />

                            <TextView
                                android:id="@+id/popup_duty"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="0dp"
                                android:layout_marginLeft="5dp"
                                android:layout_marginTop="0dp"
                                android:fontFamily="@font/nanumsquareeb"
                                android:text="singer"
                                android:textColor="#373737"
                                android:textSize="21sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15dp"
                            android:layout_marginTop="20dp"
                            android:gravity="center_vertical">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@drawable/new_field_phone" />

                            <TextView
                                android:id="@+id/popup_phone"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_below="@+id/passwordLayout"
                                android:layout_marginLeft="10dp"
                                android:layout_marginTop="0dp"
                                android:fontFamily="@font/nanumsquareeb"
                                android:text="010-0100-0100"
                                android:textColor="#49250B"
                                android:textSize="17sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="15dp"
                            android:layout_marginTop="6dp"
                            android:gravity="center_vertical">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@drawable/new_field_fax" />

                            <TextView
                                android:id="@+id/popup_mail"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="10dp"
                                android:layout_marginTop="0dp"
                                android:fontFamily="@font/nanumsquareeb"
                                android:gravity="center_vertical"
                                android:text="minhyeong@dglab.kr"
                                android:textAlignment="center"
                                android:textColor="#49250B"
                                android:textSize="17sp" />
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="80dp"
                            android:layout_marginLeft="15dp"
                            android:layout_marginTop="6dp">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:src="@drawable/new_field_work" />

                            <ScrollView
                                android:layout_width="180dp"
                                android:layout_height="75dp"
                                android:layout_below="@+id/mail"
                                android:layout_marginLeft="10dp"
                                android:layout_marginTop="3dp"
                                android:layout_toEndOf="@+id/mission">

                                <TextView
                                    android:id="@+id/popup_mission"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="2dp"
                                    android:ellipsize="none"
                                    android:fontFamily="@font/nanumsquareeb"
                                    android:maxLines="100"
                                    android:scrollHorizontally="false"
                                    android:scrollbars="vertical"
                                    android:text="자치행정과"
                                    android:textColor="#49250B"
                                    android:textSize="17sp" />
                            </ScrollView>
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="40dp"
                    android:paddingTop="25dp"
                    android:visibility="gone">

                    <TextView
                        android:id="@+id/working"
                        android:layout_width="96dp"
                        android:layout_height="29dp"
                        android:background="@drawable/new_status_1_off"
                        android:visibility="visible" />

                    <TextView
                        android:id="@+id/vacation"
                        android:layout_width="96dp"
                        android:layout_height="29dp"
                        android:layout_marginLeft="12dp"
                        android:background="@drawable/new_status_2_off"
                        android:visibility="visible" />

                    <TextView
                        android:id="@+id/trip"
                        android:layout_width="96dp"
                        android:layout_height="29dp"
                        android:layout_marginLeft="12dp"
                        android:background="@drawable/new_status_3_off"
                        android:visibility="visible" />

                    <TextView
                        android:id="@+id/education"
                        android:layout_width="96dp"
                        android:layout_height="29dp"
                        android:layout_marginLeft="12dp"
                        android:background="@drawable/new_status_4_off"
                        android:visibility="visible" />
                </LinearLayout>
            </LinearLayout>

            <RelativeLayout
                android:id="@+id/passwordLayout"
                android:layout_width="494dp"
                android:layout_height="194dp"
                android:background="@drawable/new_id_card_sub_bg"
                android:paddingLeft="50dp"
                android:paddingRight="50dp"
                android:paddingTop="20dp"
                android:visibility="gone">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/password_field"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:background="@drawable/password_round"
                        android:gravity="center_vertical"
                        android:hint="Password"
                        android:paddingLeft="10dp"
                        android:textColor="#9e9e9e"
                        android:textSize="18dp" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <TextView
                            android:id="@+id/p1"
                            android:layout_width="55dp"
                            android:layout_height="51dp"
                            android:layout_alignParentStart="true"
                            android:layout_alignParentTop="true"
                            android:layout_marginLeft="-7dp"
                            android:layout_marginTop="4dp"
                            android:background="@drawable/new_num_out"
                            android:gravity="center_vertical"
                            android:text=" 1"
                            android:textAlignment="center"
                            android:textColor="#333333"
                            android:textSize="30dp" />

                        <TextView
                            android:id="@+id/p2"
                            android:layout_width="55dp"
                            android:layout_height="51dp"
                            android:layout_alignTop="@id/p1"
                            android:layout_toEndOf="@+id/p1"
                            android:background="@drawable/new_num_out"
                            android:gravity="center_vertical"
                            android:text=" 2"
                            android:textAlignment="center"
                            android:textColor="#333333"
                            android:textSize="30dp" />

                        <TextView
                            android:id="@+id/p3"
                            android:layout_width="55dp"
                            android:layout_height="51dp"
                            android:layout_alignTop="@id/p1"
                            android:layout_toEndOf="@+id/p2"
                            android:background="@drawable/new_num_out"
                            android:gravity="center_vertical"
                            android:text=" 3"
                            android:textAlignment="center"
                            android:textColor="#333333"
                            android:textSize="30dp" />

                        <TextView
                            android:id="@+id/p4"
                            android:layout_width="55dp"
                            android:layout_height="51dp"
                            android:layout_alignTop="@id/p1"
                            android:layout_toEndOf="@+id/p3"
                            android:background="@drawable/new_num_out"
                            android:gravity="center_vertical"
                            android:text=" 4"
                            android:textAlignment="center"
                            android:textColor="#333333"
                            android:textSize="30dp" />

                        <TextView
                            android:id="@+id/p5"
                            android:layout_width="55dp"
                            android:layout_height="51dp"
                            android:layout_alignTop="@id/p1"
                            android:layout_toEndOf="@+id/p4"
                            android:background="@drawable/new_num_out"
                            android:gravity="center_vertical"
                            android:text=" 5"
                            android:textAlignment="center"
                            android:textColor="#333333"
                            android:textSize="30dp" />

                        <TextView
                            android:id="@+id/undo"
                            android:layout_width="104dp"
                            android:layout_height="45dp"
                            android:layout_alignTop="@id/p1"
                            android:layout_marginLeft="30dp"
                            android:layout_marginTop="3dp"
                            android:layout_toEndOf="@+id/p5"
                            android:background="@drawable/new_btn_backspace"
                            android:gravity="center_vertical"
